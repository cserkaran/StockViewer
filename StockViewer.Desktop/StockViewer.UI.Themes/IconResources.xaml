<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/StockViewer.UI.Themes;component/Geometry.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <!--#region Styles-->

    <Style x:Key="SxPathxIcon" TargetType="{x:Type Path}">
        <Setter Property="UseLayoutRounding" Value="False"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Stretch" Value="Fill"/>
    </Style>

    <Style x:Key="SxPathxIconx16" TargetType="{x:Type Path}" BasedOn="{StaticResource SxPathxIcon}">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
    </Style>


    <!--#endregion-->
    
    <!--#region Icons Resources-->

    <Path x:Key="IxSearch" x:Shared="False" Style="{StaticResource SxPathxIconx16}" Data="{StaticResource GxSearch}" >
    </Path>
    
    <!--#endregion-->
    
</ResourceDictionary>